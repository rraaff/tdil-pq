<ibatorContext
  id="ibatorContext"
  targetRuntime="Ibatis2Java5"
  defaultModelType="flat">

<ibatorPlugin type="com.tdil.ibatis.plugin.MySqlDeleteByExamplePlugin">
</ibatorPlugin>

<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/PEUGEOTSVC" userId="PEUGEOTSVC_USER"
			password="PEUGEOTSVC_USER" />


<javaModelGenerator targetPackage="com.tdil.ljpeugeot.model"
  targetProject="PeugeotServiceWeb">
  <property name="enableSubPackages" value="true" />
  <property name="trimStrings" value="true" />
  <property name="rootClass" value="com.tdil.ibatis.PersistentObject" />
</javaModelGenerator>

<sqlMapGenerator targetPackage="com.tdil.ljpeugeot.dao.maps"
  targetProject="PeugeotServiceWeb">
  <property name="enableSubPackages" value="true" />
</sqlMapGenerator>

<daoGenerator type="GENERIC-CI"
targetPackage="com.tdil.ljpeugeot.dao"
implementationPackage="com.tdil.ljpeugeot.dao.impl"
targetProject="PeugeotServiceWeb">
  <property name="enableSubPackages" value="true" />
  <property name="methodNameCalculator" value="extended" />
</daoGenerator>
