<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
 <display-name>GISWeb</display-name>
 <servlet>
  <servlet-name>CreateAsyncJob</servlet-name>
  <servlet-class>com.gis.emulator.CreateAsyncJob</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>CreateAsyncJob</servlet-name>
  <url-pattern>/gis/activateAlarm.json</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CreateAsyncJob</servlet-name>
  <url-pattern>/gis/deactivateAlarm.json</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CreateAsyncJob</servlet-name>
  <url-pattern>/gis/turnOnLight.json</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CreateAsyncJob</servlet-name>
  <url-pattern>/gis/turnOffLight.json</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CreateAsyncJob</servlet-name>
  <url-pattern>/gis/sendPanic.json</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CreateAsyncJob</servlet-name>
  <url-pattern>/gis/activateLightRandomSequence.json</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CreateAsyncJob</servlet-name>
  <url-pattern>/gis/deactivateLightRandomSequence.json</url-pattern>
 </servlet-mapping>
</web-app>
