body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,blockquote,th,p {
	margin:0;
	padding:0;
}
body{
	background-image: url(../images/fondoFE.jpg);
	background-repeat: no-repeat;
	background-position: center top;
	margin: 0px;
	padding: 0px;
	font-family: "Univers LT", Geneva, Arial, Helvetica, sans-serif;
	font-size: 12px;
	line-height: normal;
	color: #FFFFFF;
}
html, body {
	height: 100%;
	overflow: hidden;
}
a, a:active, a:visited {
	color:#aa0410;
}
a:hover {
	color:#aa0410;
	text-decoration:underline;
}
img {
	border:none;
}
input, input:hover, input:active, input:visited {
	border: none;
}
.remarcado {
	color:#aa0410;
}
#centralContent {
	height: 600px;
	width: 900px;
	margin-right: auto;
	margin-left: auto;
	margin-top: 0px;
	margin-bottom: 0px;
	padding: 0px;
}
#promoButtons {
	height: 90px;
	width: 336px;
	margin-top: 400px;
	margin-right: 239px;
	margin-left: 325px;
	padding-top: 0px;
	padding-right: 0px;
	padding-bottom: 0px;
	padding-left: 0px;
	margin-bottom: 0px;
}
#centralContent #footer {
	margin-top: 70px;
}
#centralContent #footer #legal {
	font-family: "Univers LT", Geneva, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #464646;
	width: 200px;
	float: left;
	margin-top: 12px;
	margin-left: 395px;
	text-align: center;
}
#centralContent #footer #legal a {
	color: #464646;
}
#centralContent #footer #legal a:hover, #centralContent #footer #legal a:active, #centralContent #footer #legal a:visited {
	color:#aa0410;
	text-decoration: underline;
}
#centralContent #footer #socialHub {
	float: right;
	height: 30px;
	width: 213px;
	margin-right: 14px;
}

#basic-modal-registro {display:none;}
#basic-modal-login {display:none;}
#basic-modal-cargaCodigo {display:none;}
#basic-modal-recordarPassword {display:none;}
#basic-modal-gracias {display:none;}
#basic-modal-win {display:none;}
#basic-modal-claveEnviada {display:none;}
#basic-modal-graciasPorRegistrarte {display:none;}
#basic-modal-compartirEmail {display:none;}
#basic-modal-compartirEmailGracias {display:none;}

/* Login */
/* Overlay */
#login-overlay {background-color:#000; cursor:wait;}
/* Container */
#login-container {
	height:403px;
	width:473px;
	margin-left:50px;
	filter:alpha(opacity=85); /* IE's opacity*/
	opacity: 0.85;
	background-image: url(../images/interfases/bgLogin.png);
	background-repeat: no-repeat;
	background-position: left top;
}
#login-container .simplemodal-data {
	padding:50px;
	margin-top: 70px;
}
#login-container input {
	background-color: transparent;
	border: none;
	width:280px;
	color:#aa0410;
	font-size: 14px;
	line-height: normal;
}
#basic-modal-login #loginForm .loginButton {
	background-image: url(../images/buttons/enviarDatos.png);
	background-repeat: no-repeat;
	background-position: center center;
	height: 56px;
	width: 168px;
	cursor: hand;
	font-size: 10px;
	margin: 0px;
	padding: 0px;
	/*border:1px dashed #212021;*/
	border:none;
}
#login-container code { color:#bbb; display:block; font-size:12px; margin-bottom:12px; padding:4px 6px 6px;}
/*#login-container a {color:#ddd;}*/
#login-container a.modalCloseImg {background:url(../images/buttons/botonX.png) no-repeat; width:36px; height:37px; display:inline; z-index:3200; position:absolute; top:18px; right:35px; cursor:pointer;}
#login-container h3 {color:#d90e1c;}

