<select id="selectExperience" resultMap="EXPERIENCE.BaseResultMap">
    select 
    	pi.id as exp_id, 'POST_ITS' as exp_type, pi.title as exp_description,
    	pi.id_image as exp_idblob, pi.ext_image as exp_extblob, 
    	(select COUNT(*) FROM LINKS where destinationType = 'POST_ITS' and destination = pi.id) as exp_linkCount
    from POST_IT pi
    where pi.approved = 1
    order by exp_linkCount asc, pi.id desc
  </select>
  
