<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ibatorConfiguration
  PUBLIC "-//Apache Software Foundation//DTD Apache iBATIS Ibator Configuration 1.0//EN"
  "http://ibatis.apache.org/dtd/ibator-config_1_0.dtd">

<ibatorConfiguration>
<classPathEntry location="/home/mgodoy/icarus/workspace/thalamus/thalamusJClient/EnvironmentLibraries/mysql-connector-java-5.1.13-bin.jar" />
<classPathEntry location="/home/mgodoy/icarus/workspace/thalamus/thalamusJClient/EnvironmentLibraries/javabase.jar" />
<classPathEntry location="/home/mgodoy/icarus/workspace/thalamus/thalamusJClient/EnvironmentLibraries/pluginibator.jar" />
<ibatorContext 
  id="ibatorContext" 
  targetRuntime="Ibatis2Java5" 
  defaultModelType="flat">

<ibatorPlugin type="com.tdil.ibatis.plugin.MySqlDeleteByExamplePlugin">
</ibatorPlugin>

<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/LOJACK" userId="LOJACK_USER"
			password="LOJACK_USER" />


<javaModelGenerator targetPackage="com.tdil.lojack.model"
  targetProject="LoJackWeb">
  <property name="enableSubPackages" value="true" />
  <property name="trimStrings" value="true" />
  <property name="rootClass" value="com.tdil.ibatis.PersistentObject" />
</javaModelGenerator>

<sqlMapGenerator targetPackage="com.tdil.lojack.dao.maps"
  targetProject="LoJackWeb">
  <property name="enableSubPackages" value="true" />
</sqlMapGenerator>

<daoGenerator type="GENERIC-CI" 
targetPackage="com.tdil.lojack.dao"
implementationPackage="com.tdil.lojack.dao.impl"
targetProject="LoJackWeb">
  <property name="enableSubPackages" value="true" />
  <property name="methodNameCalculator" value="extended" />
</daoGenerator>

<table schema="LOJACK" tableName="WEBSITEUSER" domainObjectName="WebsiteUser" alias="su">
	<generatedKey column="id" sqlStatement="MySql" identity="true" type="post"/>
</table>
<table schema="LOJACK" tableName="SYSPROPERTIES" domainObjectName="SystemProperty" alias="sp">
	<generatedKey column="id" sqlStatement="MySql" identity="true" type="post"/>
</table>
<table schema="LOJACK" tableName="BLOB_DATA" domainObjectName="BlobData" alias="bd">
	<generatedKey column="id" sqlStatement="MySql" identity="true" type="post"/>
</table>

<table schema="LOJACK" tableName="CACHE_REGION" domainObjectName="CacheRegion" alias="cache">
	<generatedKey column="id" sqlStatement="MySql" identity="true" type="post"/>
</table>


</ibatorContext>
</ibatorConfiguration>