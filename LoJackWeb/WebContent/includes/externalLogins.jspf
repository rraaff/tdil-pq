<%@page import="com.tdil.lojack.utils.LoJackWebUtils"%>
<% if (websiteUser != null && websiteUser.isLogged() && websiteUser.isPetUser()) { %>
	<% if (!LoJackWebUtils.isMobile(request)){ %>
		$( "#enterPets" ).click(function() {
			var userDate = new Date();
			var userTimeZone = ( userDate.getTimezoneOffset()/60 )*( -1 );
			window.open('<%=com.tdil.lojack.pets.PetsConnector.getPetsLoginUrl()%>SESSIONID=<%=websiteUser.getJSESSIONID()%>&TIMEZONEOFFSET=' +userTimeZone+ '&LOJACKTOKEN=<%=com.tdil.lojack.pets.PetsConnector.getPetsToken()%>&AWSELB=<%=websiteUser.getAWSELB()%>', 'Lojack Pets');
		});
	<% } else { %>
		$( "#enterPets" ).click(function() {
			var userDate = new Date();
			var userTimeZone = ( userDate.getTimezoneOffset()/60 )*( -1 );
			window.open('<%=com.tdil.lojack.pets.PetsConnector.getPetsMobileLoginUrl()%>SESSIONID=<%=websiteUser.getJSESSIONID()%>&TIMEZONEOFFSET=' +userTimeZone+ '&LOJACKTOKEN=<%=com.tdil.lojack.pets.PetsConnector.getPetsToken()%>&AWSELB=<%=websiteUser.getAWSELB()%>', 'Lojack Pets');
		});
	<% } %>
<% } %>
<% if (websiteUser != null && websiteUser.isLogged() && websiteUser.isPreventUser()) { %>
	<% if (usingMobile || isAndroid){ %>
		$( "#enterPrevent" ).click(function() {
			var userDate = new Date();
			var userTimeZone = ( userDate.getTimezoneOffset()/60 )*( -1 );
			window.open('<%=com.tdil.lojack.prevent.PreventConnector.getPreventLoginUrl()%>SESSIONID=<%=websiteUser.getJSESSIONID()%>&TIMEZONEOFFSET=' +userTimeZone+ '&LOJACKTOKEN=<%=com.tdil.lojack.prevent.PreventConnector.getPreventToken()%>&AWSELB=<%=websiteUser.getAWSELB()%>', 'Lojack Prevent');
		});
	<% } %>
<% } %>