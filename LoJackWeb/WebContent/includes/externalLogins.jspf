<%@page import="com.tdil.lojack.utils.LoJackWebUtils"%>
<% if (websiteUser != null && websiteUser.isLogged() && websiteUser.isPetUser()) { %>
	<% if (!usingMobile && !isAndroid){ %>
		$( "#enterPets" ).click(function() {
		<%@ include file="blockUI.jspf" %>
		$.ajax({
          type: "GET",
          cache: false,
          url: "./refreshThalamusLoginCacheServlet.st",
          dataType: "json",
          success: function(data) {
        	  <%@ include file="unblockUI.jspf" %>
        	  if (data.result == 'OK') {
					var userDate = new Date();
					var userTimeZone = ( userDate.getTimezoneOffset()/60 )*( -1 );
					window.open('<%=com.tdil.lojack.pets.PetsConnector.getPetsLoginUrl(websiteUser)%>SESSIONID=<%=websiteUser.getJSESSIONID()%>&TIMEZONEOFFSET=' +userTimeZone+ '&LOJACKTOKEN=<%=com.tdil.lojack.pets.PetsConnector.getPetsToken()%>&AWSELB=<%=websiteUser.getAWSELB()%>', 'Lojack_Pets');
				} else {
					errorAjax();
				}
       	   },
	          error: function() {
	        	  <%@ include file="unblockUI.jspf" %>
	        	  errorAjax();
	          }
	      });
		});
	<% } else { %>
		$( "#enterPets" ).click(function() {
			$.ajax({
	          type: "GET",
	          cache: false,
	          url: "./refreshThalamusLoginCacheServlet.st",
	          dataType: "json",
	          success: function(data) {
	        	  <%@ include file="unblockUI.jspf" %>
	        	  if (data.result == 'OK') {
						var userDate = new Date();
						var userTimeZone = ( userDate.getTimezoneOffset()/60 )*( -1 );
						window.open('<%=com.tdil.lojack.pets.PetsConnector.getPetsMobileLoginUrl(websiteUser)%>SESSIONID=<%=websiteUser.getJSESSIONID()%>&TIMEZONEOFFSET=' +userTimeZone+ '&LOJACKTOKEN=<%=com.tdil.lojack.pets.PetsConnector.getPetsToken()%>&AWSELB=<%=websiteUser.getAWSELB()%>', 'Lojack_Pets');
					} else {
						errorAjax();
					}
		          },
		          error: function() {
		        	  <%@ include file="unblockUI.jspf" %>
		        	  errorAjax();
		          }
		      });
		});
	<% } %>
<% } %>
